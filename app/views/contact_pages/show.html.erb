<div class="contact-container">
  <div class="container mt-4">
    <div class="row">
      <div class="col-md-4 offset-md-3 text-center">
        <h1><%= @contact_page.title %></h1>
        <ul class="list-unstyled">
          <li><strong>Phone:</strong> <%= @contact_page.phone %></li>
          <li><strong>Email:</strong> <%= @contact_page.email %></li>
          <li><strong>Address:</strong> <%= @contact_page.address %></li>
        </ul>
      </div>
    </div>
  </div>
</div>
